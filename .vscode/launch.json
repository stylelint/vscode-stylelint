{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"preLaunchTask": "Bundle Extension (Watch)",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
				"${workspaceRoot}/test/e2e/workspace/workspace.code-workspace",
				"--disable-extensions",
				"--trace-warnings"
			],
			"outFiles": [
				"${workspaceFolder}/**/*.(m|c|)js",
				"!**/node_modules/**"
			]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Server",
			"address": "localhost",
			"protocol": "inspector",
			"port": 6004,
			"outFiles": [
				"${workspaceFolder}/**/*.(m|c|)js",
				"!**/node_modules/**"
			]
		},
		{
			"name": "Launch Tests",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
				"--extensionTestsPath=${workspaceRoot}/test"
			],
			"stopOnEntry": false
		}
	]
}
