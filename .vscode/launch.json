{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"preLaunchTask": "Bundle Extension (Watch)",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
				"${workspaceRoot}/test/e2e/workspace/workspace.code-workspace",
				"--disable-extensions",
				"--trace-warnings"
			],
			"env": { "NODE_ENV": "development" },
			"stopOnEntry": false
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Server",
			"port": 6004,
			"restart": true,
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/dist/start-server.js"]
		},
		{
			"name": "Launch Tests",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
				"--extensionTestsPath=${workspaceRoot}/test"
			],
			"stopOnEntry": false
		}
	]
}
