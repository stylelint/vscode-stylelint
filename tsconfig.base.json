{
	"compilerOptions": {
		"composite": true,
		"incremental": true,
		"target": "ES2022",
		"module": "nodenext",
		"lib": ["es2023"],
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true
	}
}
